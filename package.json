{
	"name": "dataview-properties",
	"version": "0.0.24",
	"description": "Automagically copy dataview inline field (and their values, even calculated!) into frontmatter properties and keep them sync.",
	"main": "main.js",
	"private": true,
	"scripts": {
		"prebuild": "tsc --noEmit --skipLibCheck",
		"build": "node esbuild.config.mjs --production",
		"dev": "node esbuild.config.mjs -v",
		"export": "node esbuild.config.mjs --production --vault",
		"bump": "node commit-and-tag-version.mjs",
		"postbump": "git push --follow-tags origin master",
		"predeploy": "bun run bump",
		"deploy": "bun run export",
		"lint": "bun biome check --write src/",
		"pree2e": "bun run build",
		"e2e": "wdio run ./wdio.conf.mts"
	},
	"engines": {
		"node": "^22.2.0"
	},
	"keywords": [],
	"author": "Mara-Li",
	"license": "GPL-3.0",
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "^1.3.6",
		"@types/luxon": "^3.7.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^25.0.8",
		"@wdio/cli": "^9.23.0",
		"@wdio/local-runner": "^9.23.0",
		"@wdio/mocha-framework": "^9.23.0",
		"@wdio/spec-reporter": "^9.20.0",
		"builtin-modules": "5.0.0",
		"esbuild": "0.27.2",
		"i18next": "^25.7.4",
		"obsidian": "^1.11.4",
		"ts-deepmerge": "^7.0.3",
		"tslib": "2.8.1",
		"typescript": "^5.9.3",
		"wdio-obsidian-reporter": "^2.2.0",
		"wdio-obsidian-service": "^2.2.0"
	},
	"dependencies": {
		"@delucis/if-env": "^1.1.2",
		"@enveloppe/obsidian-dataview": "^0.5.70-2",
		"ansi-colors": "^4.1.3",
		"commander": "^14.0.2",
		"commit-and-tag-version": "^12.6.1",
		"dedent": "^1.7.1",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^11.0.0",
		"mocha": "^11.7.5",
		"obsidian-typings": "^4.93.0",
		"uniformize": "^2.5.0",
		"update": "^0.7.4"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"classic-level",
		"edgedriver",
		"electron",
		"esbuild",
		"geckodriver",
		"obsidian-typings"
	],
	"overrides": {
		"luxon": "^3.7.1"
	}
}